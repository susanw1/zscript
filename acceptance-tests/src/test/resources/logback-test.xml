<configuration debug="false">
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level [%-16thread] %logger{48} - %msg%n</pattern>
        </encoder>
    </appender>

    <!--    <logger name="net.zscript" level="trace"/>-->

    <root level="info">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
